runLitpScript cluster_common.atlib

litp create -p /infrastructure/networking/networks/traffic1 -t network -o name=traffic1
litp create -p /deployments/test/clusters/cluster1/nodes/node_1/network_interfaces/nic_4  -t eth -o device_name=eth4 macaddress=08:00:27:21:7D:B2 network_name=traffic1 ipv6address=2001:abcd:ef::10/64
litp create -p /deployments/test/clusters/cluster1/nodes/node_2/network_interfaces/nic_4  -t eth -o device_name=eth4 macaddress=08:00:27:21:7D:B1 network_name=traffic1

litp create -t service -p /software/services/mock -o service_name=mocklsb
litp create -t vcs-clustered-service -p /deployments/test/clusters/cluster1/services/cs1 -o name="cs1" active=1 standby=1 node_list="node_1,node_2"
litp inherit -s /software/services/mock -p /deployments/test/clusters/cluster1/services/cs1/applications/mock

# Positive1: create ipv6 vips for failoverAPP1 and parallelAPP2:
litp create -p /deployments/test/clusters/cluster1/services/cs1/ipaddresses/ip6_1 -t vip -o  ipaddress='2001:abcd:ef::10/64'  network_name='mgmt'
litp create -p /deployments/test/clusters/cluster1/services/cs1/ipaddresses/ip6_2 -t vip -o  ipaddress='2001:abcd:ef::8/64'  network_name='mgmt'

let __err_1_node1 --err_message 'Overlapping network subnet defined on node "mn1"' --err_vpath /deployments/test/clusters/cluster1/services/cs1/ipaddresses/ip6_1
let __err_2_node1 --err_message 'Overlapping network subnet defined on node "mn1"' --err_vpath /deployments/test/clusters/cluster1/services/cs1/ipaddresses/ip6_2
let __err_network_node1 --err_message 'Overlapping network subnet defined on node "mn1"' --err_vpath /deployments/test/clusters/cluster1/nodes/node_1/network_interfaces/nic_4

assertError __err_1_node1 __err_2_node1 __err_network_node1 --errors_length 3 create_plan

# If there is overlapping in the node, it will be ignored because will be generated by network_plugin
litp create -p /deployments/test/clusters/cluster1/nodes/node_1/network_interfaces/nic_5  -t eth -o device_name=eth5 macaddress=08:00:27:21:7D:F2 network_name=mgmt ipv6address=2001:abcd:e1::10/64
litp create -p /deployments/test/clusters/cluster1/nodes/node_2/network_interfaces/nic_5  -t eth -o device_name=eth5 macaddress=08:00:27:21:7D:F1 network_name=mgmt

assertError __err_1_node1 __err_2_node1 --errors_length 2 create_plan
